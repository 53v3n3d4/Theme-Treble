{
    "extends": "colors-palette.hidden-theme",
    "variables":
    {
        // Light and Dark variable names refer to Light and Dark color scheme background
        // Lighter and Darker Tone variable names refer to tonality

        // Font size
        // 1.2rem = 13
        "font-size-sidebar-heading":                     "1.0rem",
        "font-size-sidebar-label":                       "1.1rem",
        "font-size-quick-panel-label-key-binding":       "1.2rem",
        "font-size-quick-panel-label-hint":              "1.1rem",
        "font-size-quick-panel-label":                   "1.2rem",
        "font-size-kind-label":                          "1.3rem",
        "font-size-kind-label-auto-complete":            "1.3rem", // Letter inside kind container auto complete popup
        "font-size-auto-complete-label":                 "1.0rem",
        "font-size-auto-complete-kind-label":            "1.0rem",
        "font-size-auto-complete-hint":                  "1.0rem",
        "font-size-auto-complete-details":               "1.0rem",
        "font-size-text-line-control-overlay":           "1.2rem",
        "font-size-text-line-control-switch-project":    "1.2rem",
        "font-size-label-control":                       "1.0rem",
        "font-size-label-control-status-bar":            "1.0rem",
        "font-size-title-label-control":                 "2.0rem",
        "font-size-tool-tip-label-control":              "1.0rem",

        // Title bar
        "title-bar-bg":          "var(black8)",
        "title-bar-fg-dark":     "color(var(gray3) a(0.9))", // fix Update windows not work file_light
        "title-bar-fg-light":    "color(var(bg-color) blend(black 44%))",

        // Title bar Theme opacity setting
        "title-bar-fg-medium-dark":     "color(var(gray4) a(0.5))", // fix Update windows not work file_light
        "title-bar-fg-medium-light":    "color(var(bg-color) blend(var(black1) 60%))",
        "title-bar-fg-low-dark":        "color(var(black4) a(0.5))", // fix Update windows not work file_light
        "title-bar-fg-low-light":       "color(var(bg-color) blend(var(black1) 77%))",

        // Sidebar
        "sidebar-container-bg":           "var(black7)",
        "sidebar-container-bg-border":    "color(var(black5) a(0.2))",
        "tree-row-hover":                 "color(var(white1) a(0.03))",

        // Sidebar and Tab label
        "heading":                       "var(gray2)",
        "label-normal":                  "color(var(white1) a(0.6))",
        "label-hover":                   "var(white2)",
        "label-selected-dark":           "var(white2)",
        "label-selected-light":          "var(black2)",
        "label-hover-selected-dark":     "color(var(white2) a(0.6))",
        "label-hover-selected-light":    "color(var(black2) a(0.6))",
        "tab-label-normal-dark":         "color(var(white1) a(0.6))",
        "tab-label-normal-light":        "color(var(black1) a(0.6))",
        "tab-label-hover-dark":          "var(white2)",
        "tab-label-hover-light":         "var(black2)",

        // Sidebar and Tab label opacity
        "heading-opacity":                     1.0,
        "label-normal-opacity":                1.0,
        "label-hover-opacity":                 1.0,
        "label-selected-opacity":              1.0,
        "label-hover-selected-opacity":        1.0,
        // "tab-label-hover-selected-opacity":    0.7, // testing not using

        // Sidebar and Tab label Theme opacity setting
        "heading-opacity-medium":         0.65,
        "label-normal-opacity-medium":    0.65,
        "heading-opacity-low":            0.45,
        "label-normal-opacity-low":       0.45,

        // Sidebar disclosure, close and dirty
        "sidebar-disclosure-button":     "var(gray2)",
        "sidebar-close-button-dark":     "var(white2)",
        "sidebar-close-button-light":    "var(black2)",
        "sidebar-dirty-button":          "var(gray2)",

        // Sidebar diclosure, folder icon, file type icon, open files close/dirty, vcs status badge opacity
        "sidebar-icon-opacity-normal":            { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },
        "sidebar-icon-opacity-hover":             { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "sidebar-icon-opacity-selected":          { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "sidebar-icon-opacity-hover-selected":    { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },
        "sidebar-icon-opacity-vcs-ignored":       { "target": 0.3, "speed": 4.0, "interpolation": "smoothstep" },

        // Sidebar diclosure, folder icon, file type icon, open files close/dirty opacity 
        // Theme opacity setting
        "sidebar-icon-opacity-medium":    { "target": 0.5, "speed": 4.0, "interpolation": "smoothstep" },
        "sidebar-icon-opacity-low":       { "target": 0.3, "speed": 4.0, "interpolation": "smoothstep" },

        // Folder icon colors
        "folder-blue":             "color(var(blue3) l(+ 5%))",
        "folder-yellow":           "color(var(yellow3) l(- 18%))",
        "folder-graphite":         "var(gray2)",
        "folder-loading-dark":     "var(white2)",
        "folder-loading-light":    "var(black4)",

        // Vcs status multicolor
        "vcs-modified":                 "color(var(gold1) min-contrast(var(bg-color) 1.5))",
        "vcs-modified-darker-tone":     "var(yellow5)",
        "vcs-untracked":                "color(var(green-light3) min-contrast(var(bg-color) 2.0))",
        "vcs-untracked-darker-tone":    "var(green3)",
        "vcs-missing":                  "var(orange1)",
        "vcs-added":                    "var(blue3)",
        "vcs-added-lighter-tone":       "var(blue5)",
        "vcs-staged":                   "var(blue3)",
        "vcs-staged-lighter-tone":      "var(blue5)",
        "vcs-deleted":                  "var(red3)",
        "vcs-unmerged":                 "var(red1)",
        "vcs-unmerged-darker-tone":     "var(red2)",
        "vcs-ignored":                  "color(var(white1) a(0.3))",
        "vcs-ignored-darker-tone":      "var(gray4)",
        "vcs-ignored-lighter-tone":     "var(gray2)",

        // Vcs status grayscale
        "vcs-status-badge-dark":                        "color(var(white1) a(0.6))",
        "vcs-status-badge-light":                       "color(var(black1) a(0.6))",
        "vcs-status-badge-hover-row-dark":              "var(white2)",
        "vcs-status-badge-hover-row-light":             "var(black2)",
        "vcs-status-badge-selected-row-dark":           "var(white2)",
        "vcs-status-badge-selected-row-light":          "var(black2)",
        "vcs-status-badge-hover-selected-row-dark":     "color(var(white2) a(0.6))",
        "vcs-status-badge-hover-selected-row-light":    "color(var(black2) a(0.6))",

        // Tabs
        "tabset-control-bg":           "var(bg-color)",
        "tabset-control-bg-border":    "var(bg-color)",

        // Tabs Tab style Do and Re Tabset background setting
        "tabset-control-bg-tabset-bg":           "var(black6)",
        "tabset-control-bg-border-tabset-bg":    "color(var(black5) a(0.2))", //"var(black5)",
        "tab-control-bg-tabset-bg":              "var(black6)",

        // Tabs opacity Tabset accent setting
        "tab-selected-accent":    0.14,

        // Tab, scroll tabs, new tab and dropdown
        "tab-unselected-shadow-dark":           "color(var(bg-color) blend(white 60%))",
        "tab-unselected-shadow-light":          "color(var(bg-color) blend(black 60%))",
        "tab-unselected-dark":                  "color(var(bg-color) blend(white 95%))", //"color(var(white1) a(0.03))",
        "tab-unselected-light":                 "color(var(bg-color) blend(black 95%))", //"color(var(black1) a(0.03))",
        "tab-unselected-transparent":           "transparent",
        "tab-unselected-bg":                    "var(bg-color)",
        "tab-hover-dark":                       "color(var(bg-color) blend(white 90%))", //"color(var(white1) a(0.1))",
        "tab-hover-light":                      "color(var(bg-color) blend(black 90%))", //"color(var(black1) a(0.07))",
        "tab-hover-transparent":                "transparent",
        "tab-selected-dark":                    "color(var(bg-color) blend(white 90%))", //"color(var(white1) a(0.1))",
        "tab-selected-light":                   "color(var(bg-color) blend(black 90%))", //"color(var(black1) a(0.07))",
        "tab-selected-transparent":             "transparent",
        "tab-modified-dirty-dark":              "var(gray4)",
        "tab-modified-dirty-light":             "var(gray2)",
        "tab-divider-dark":                     "var(gray4)",
        "tab-divider-light":                    "var(gray2)",
        "tab-modified-added":                   "var(green2)", // green2 to diff from accent color green
        "tab-modified-deleted":                 "var(vcs-deleted)",

        // Tab, scroll tabs, new tab and dropdown Tabset background setting
        "tab-hover-tab-style-do-tabset-bg-dark":           "color(var(white1) a(0.067))",
        "tab-hover-tab-style-do-tabset-bg-light":          "color(var(black1) a(0.09))",
        "tab-unselected-shadow-tab-style-re-tabset-bg":    "var(black1)",
        "tab-unselected-tab-style-re-tabset-bg":           "color(var(black6) blend(white 95%))", //"color(var(white1) a(0.03))",
        "tab-hover-tab-style-re-tabset-bg":                "color(var(black6) blend(white 90%))", //"color(var(white1) a(0.1))",
        "tab-selected-tab-style-re-tabset-bg":             "var(black7)",
        "tab-hover-tabset-bg":                             "color(var(black6) blend(white 90%))",

        // Tab, scroll tabs, new tab and dropdown opacity
        "tab-icon-normal-opacity":                1,
        "tab-button-bg-normal-opacity":           1,
        "tab-button-bg-border-normal-opacity":    1, // not using

        // Tab, scroll tabs, new tab and dropdown Tabset background setting
        "tab-selected-tabset-bg":    "var(bg-color)",

        // Tab, scroll tabs, new tab and dropdown opacity Theme opacity setting
        "tab-icon-normal-opacity-medium":    0.65,
        "tab-icon-normal-opacity-low":       0.40,

        // Tab close button
        "tab-x-button-normal":                     "var(gray4)",
        "tab-x-button-hover-dark":                 "var(white2)",
        "tab-x-button-hover-light":                "var(black2)",
        "tab-square-circle-button-hover-dark":     "color(var(white1) a(15%))",
        "tab-square-circle-button-hover-light":    "color(var(black1) a(10%))",
        "tab-dirty-button":                        "var(gray3)",

        // Tab close button opacity
        "tab-x-button-opacity":          { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "tab-x-button-opacity-hover":    { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "tab-dirty-button-opacity":      { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },

        // Tab close button opacity Theme opacity setting
        "tab-x-button-opacity-medium":        { "target": 0.7, "speed": 4.0, "interpolation": "smoothstep" },
        "tab-x-button-opacity-low":           { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },
        "tab-dirty-button-opacity-medium":    { "target": 0.7, "speed": 4.0, "interpolation": "smoothstep" },
        "tab-dirty-button-opacity-low":       { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },

        // Tab chevron, plus and dropdown icon
        "tab-chevron-icon":                "var(gray4)",
        "tab-chevron-icon-hover-dark":     "var(white2)",
        "tab-chevron-icon-hover-light":    "var(black2)",
        "tab-plus-icon":                   "var(gray4)",
        "tab-plus-icon-hover-dark":        "var(white2)",
        "tab-plus-icon-hover-light":       "var(black2)",

        // Accent colors
        "accent-blue":            "var(blue1)",
        "accent-purple":          "var(purple1)",
        "accent-pink":            "var(pink-light3)",
        "accent-red":             "var(red1)",
        "accent-orange":          "var(orange2)",
        "accent-yellow":          "color(var(yellow3) min-contrast(var(bg-color) 1.7))",
        "accent-green":           "color(var(green-light5) min-contrast(var(bg-color) 2.0))",
        "accent-white":           "var(white2)",
        "accent-black":           "var(black2)",
        "accent-color-scheme":    "var(--accent)",

        // Accent overlay - selected row
        "accent-white-selected-row":    "color(var(black2) a(0.1))",
        "accent-black-selected-row":    "color(var(white2) a(0.2))",

        // Overlay
        // Using dark and light, because it shows on top of color-scheme
        "overlay-shadow-dark":               "color(var(black1) a(0.4))",
        "overlay-shadow-light":              "color(var(black1) a(0.4))",
        "overlay-background":                "var(black3)",
        "overlay-border":                    "color(var(gray4) a(0.4))",
        "overlay-border-no-shadow-dark":     "var(black4)",
        "overlay-border-no-shadow-light":    "var(gray4)",

        // Kind color
        "kind-color-red":       "var(red3)",
        "kind-color-orange":    "color(var(orange4) l(- 7%))",
        "kind-color-yellow":    "color(var(gold1) l(- 17%))",
        "kind-color-green":     "var(green5)",
        "kind-color-cyan":      "var(green5)",
        "kind-color-blue":      "var(blue4)",
        "kind-color-purple":    "var(purple4)",
        "kind-color-pink":      "var(pink1)",
        "kind-color-dark":      "var(gray1)",
        "kind-color-light":     "var(black4)",

        // Kind label
        "kind-function-color":      "var(kind-color-red)",
        "kind-keyword-color":       "var(kind-color-pink)",
        "kind-markup-color":        "var(kind-color-orange)",
        "kind-namespace-color":     "var(kind-color-blue)",
        "kind-navigation-color":    "var(kind-color-yellow)",
        "kind-snippet-color":       "var(kind-color-green)",
        "kind-type-color":          "var(kind-color-purple)",
        "kind-variable-color":      "var(kind-color-cyan)",

        // Kind container
        "kind-container-red":       "var(red4)",
        "kind-container-pink":      "var(pink-light2)",
        "kind-container-orange":    "color(var(orange1) l(+ 11%))",
        "kind-container-blue":      "var(blue-light4)",
        "kind-container-yellow":    "color(var(yellow-light1) l(- 17%))",
        "kind-container-green":     "var(green-light4)",
        "kind-container-purple":    "var(purple1)",
        "kind-container-cyan":      "var(cyan1)",
        "kind-container-dark":      "var(black4)",
        "kind-container-light":     "var(gray2)",

        // Kind container opacity
        "kind-container-opacity-normal":      { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-container-opacity-selected":    { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-label-opacity-normal":          { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-label-opacity-selected":        { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },

        // Kind container opacity Theme opacity setting
        "kind-container-opacity-medium":    { "target": 0.65, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-container-opacity-low":       { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-label-opacity-medium":        { "target": 0.65, "speed": 4.0, "interpolation": "smoothstep" },
        "kind-label-opacity-low":           { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },

        // Quick panel
        "quick-panel-row-bg":    "var(black3)",

        // Quick panel label
        "quick-panel-label-fg":                      "color(var(white1) a(0.6))",
        "quick-panel-label-selected-dark":           "var(white2)",
        "quick-panel-label-selected-match-dark":     "var(white1)",
        "quick-panel-label-selected-light":          "var(black2)",
        "quick-panel-label-selected-match-light":    "var(black1)",
        "quick-panel-path-label":                    "color(var(gray4) a(0.7))",
        "quick-panel-label-key-binding":             "color(var(gray4) a(0.7))",
        "quick-panel-label-hint":                    "color(var(gray4) a(0.7))",
        "quick-panel-detail-label-link-color":       "var(blue5)",

        // Quick panel label Theme opacity setting
        "quick-panel-label-fg-medium":             "color(var(white1) a(0.35))",
        "quick-panel-label-fg-low":                "color(var(white1) a(0.27))",
        "quick-panel-path-label-medium":           "color(var(gray4) a(0.4))",
        "quick-panel-path-label-low":              "color(var(gray4) a(0.33))",
        "quick-panel-label-key-binding-medium":    "color(var(gray4) a(0.4))",
        "quick-panel-label-key-binding-low":       "color(var(gray4) a(0.33))",
        "quick-panel-label-hint-medium":           "color(var(gray4) a(0.4))",
        "quick-panel-label-hint-low":              "color(var(gray4) a(0.33))",

        // Sheet
        "sheet-dark-modifier":     "blend(var(black3) 80%)",
        "sheet-light-modifier":    "blend(var(black3) 97%)",

        // Views
        "text-area-modifier-dark":             "blend(var(black1) 80%)",
        "text-area-modifier-light":            "l(- 5%)",
        "text-area-console-panel-modifier":    "l(- 5%)",
        "text-area-output-panel-modifier":     "l(- 5%)",
        "text-area-dialog-modifier":           "l(- 4%)",
        "grid-border-color-dark":              "color(var(bg-color) l(+ 11%))",
        "grid-border-color-light":             "color(var(bg-color) l(- 11%))",
        "minimap-viewport-color-dark":         "color(var(bg-color) l(+ 9%))",
        "minimap-viewport-color-light":        "color(var(bg-color) l(- 7%))",
        "fold-button-color-dark":              "var(gray2)",
        "fold-button-color-light":             "var(gray3)",
        "popup-shadow-dark":                   "color(var(black1) a(0.3))",
        "popup-shadow-light":                  "color(var(black4) a(0.4))",
        "popup-html-border":                   "color(var(gray4) a(0.4))",
        "popup-control-bg":                    "var(bg-color)",

        // Views opacity
        "minimap-viewport-no-show":    { "target": 0.0, "speed": 4.0, "interpolation": "smoothstep" },
        "minimap-viewport-hover":      { "target": 1.0, "speed": 20.0, "interpolation": "smoothstep" },
        "fold-button-hover":           { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },

        // Auto complete
        "auto-complete-label-fg-dark":                   "color(var(white1) a(0.6))",
        "auto-complete-label-fg-light":                  "color(var(black1) a(0.6))",
        "auto-complete-label-selected-dark":             "var(white2)",
        "auto-complete-label-selected-match-dark":       "var(white1)",
        "auto-complete-label-selected-light":            "var(black2)",
        "auto-complete-label-selected-match-light":      "var(black1)",
        "auto-complete-hint-dark":                       "color(var(gray4) a(0.7))",
        "auto-complete-hint-light":                      "color(var(gray4) a(0.5))",
        "auto-complete-kind-name-label-border":          "var(gray4)",
        "auto-complete-detail-pane-bg-color-dark":       "color(var(white1) a(0.03))",
        "auto-complete-detail-pane-bg-color-light":      "color(var(black1) a(0.03))",
        "auto-complete-detail-pane-link-color-dark":     "var(blue5)",
        "auto-complete-detail-pane-link-color-light":    "var(blue3)",

        // Auto complete Theme opacity setting
        "auto-complete-label-fg-dark-medium":             "color(var(white1) a(0.35))",
        "auto-complete-label-fg-light-medium":            "color(var(black1) a(0.35))",
        "auto-complete-label-fg-dark-low":                "color(var(white1) a(0.27))",
        "auto-complete-label-fg-light-low":               "color(var(black1) a(0.27))",
        "auto-complete-kind-name-label-border-medium":    "color(var(gray4) a(0.7)",
        "auto-complete-kind-name-label-border-low":       "color(var(gray4) a(0.4)",

        // Auto complete opacity
        "auto-complete-hint-opacity":               1,
        "auto-complete-hint-opacity-selected":      1,
        "auto-complete-kind-name-label-opacity":    1,
        "auto-complete-detail-pane-opacity":        1,

        // Auto complete opacity Theme opacity setting
        "auto-complete-hint-opacity-medium":               0.7,
        "auto-complete-hint-opacity-low":                  0.4,
        "auto-complete-kind-name-label-opacity-medium":    0.7,
        "auto-complete-kind-name-label-opacity-low":       0.4,
        "auto-complete-detail-pane-opacity-medium":        0.7,
        "auto-complete-detail-pane-opacity-low":           0.4,

        // Table row opacity
        "table-row-accent-color-opacity":    { "target": 1.0, "speed": 20.0, "interpolation": "smoothstep" },

        // Panel
        "panel-control-bg":                         "var(black6)",
        "panel-control-switch-project-panel-bg":    "var(black3)",

        // Panel close button
        "panel-x-button-normal":               "var(gray4)",
        "panel-x-button-hover":                "var(white2)",
        "panel-square-circle-button-hover":    "color(var(white1) a(15%))",

        // Panel close button opacity
        "panel-x-button-opacity":          { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },
        "panel-x-button-opacity-hover":    { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },

        // Panel close button opacity Theme opacity setting
        "panel-x-button-opacity-medium":    { "target": 0.7, "speed": 4.0, "interpolation": "smoothstep" },
        "panel-x-button-opacity-low":       { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },

        // Status bar
        "status-bar-bg":                          "var(black5)",
        "status-bar-bg-border":                   "var(black5)",
        "vcs-changes-annotation-border":          "color(var(white1) a(0.3))",
        "vcs-changes-annotation-border-hover":    "color(var(white1) a(0.4))",
        // Maybe do a status icon and merge sidebar button and branch icon
        "sidebar-button-control":                 "color(var(white1) a(0.6))",
        "sidebar-button-control-hover":           "var(white2)",
        "branch-icon":                            "color(var(white1) a(0.65))",
        "branch-icon-hover":                      "var(white2)",

        // Status bar icon opacity - Sidebar button, Vcs branch
        "status-bar-icon-opacity-normal":    { "target": 0.8, "speed": 4.0, "interpolation": "smoothstep" },
        "status-bar-icon-opacity-hover":     { "target": 1.0, "speed": 4.0, "interpolation": "smoothstep" },

        // Status bar Theme opacity setting - Sidebar button, Vcs branch
        "status-bar-icon-opacity-medium":    { "target": 0.6, "speed": 4.0, "interpolation": "smoothstep" },
        "status-bar-icon-opacity-low":       { "target": 0.4, "speed": 4.0, "interpolation": "smoothstep" },
        "vcs-changes-annotation-border-medium":    "color(var(white1) a(0.13))",
        "vcs-changes-annotation-border-low":       "color(var(white1) a(0.07))",

        // Dialogs
        "dialog-bg":                  "var(black3)",
        "progress-bar-control-bg":    "var(gray2)",

        // Scroll bars
        "scroll-bar-control-bg-sidebar":              "var(black7)",
        "scroll-bar-control-bg-overlay":              "var(black3)",
        "scroll-track-control-dark":                  "transparent",
        "scroll-track-control-light":                 "transparent",
