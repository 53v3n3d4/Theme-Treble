{
    "extends": "light-vars.hidden-theme",
    "variables": {},
    "rules":
    [
        // class, settings, atributtes, parents
        // tint, opacity, texture, draw center, innermargin
        // font face, font size, bold, italic, color, shadow color, shadow offset, opacity
        // fg, match fg, selected fg, selected match fg, bg, match bg, font face, font size

        /**
         *
         * WINDOWS
         *
         * Title bar
         *
         */
        {
            "class": "title_bar",
            // "settings": ["themed_title_bar"],
            "bg": "var(title-bar-bg)",
            "style": "system"
        },
        // Title bar Style system Theme opacity setting
        {
            "class": "title_bar",
            "settings": {
                "themed_title_bar": true,
                "theme_opacity": "high"
            },
            "fg": "var(title-bar-fg-light)",
            "style": "system"
        },
        {
            "class": "title_bar",
            "settings": {
                "themed_title_bar": true,
                "theme_opacity": "medium"
            },
            "fg": "var(title-bar-fg-medium-light)",
            "style": "system"
        },
        {
            "class": "title_bar",
            "settings": {
                "themed_title_bar": true,
                "theme_opacity": "low"
            },
            "fg": "var(title-bar-fg-low-light)",
            "style": "system"
        },

        /**
         *
         * SIDEBAR
         *
         * Sidebar container
         *
         */
        {
            "class": "sidebar_container",
            "settings": {
                "light_theme_style": ["mix", "analog", "digital"]
            },
            "content_margin": [
                0,
                8,
                0,
                0
            ],
            "layer0.opacity": 1,
            "layer0.tint": "var(sidebar-container-bg)",
            // Layer 1: border
            "layer1.draw_center": false,
            "layer1.inner_margin": [
                0,
                1,
                0,
                0
            ], // border sidebar top
            "layer1.opacity": 1,
            "layer1.tint": "var(sidebar-container-bg-border)"
        },
        /** Sidebar tree */
        // Sidebar tree Show disclosure button setting false
        {
            "class": "sidebar_tree",
            "indent": 11,
            "indent_offset": 0,
            "indent_top_level": false,
            "spacer_rows": true // blank line between Open files and Folders
        },
        // Sidebar tree Show disclosure button setting
        {
            "class": "sidebar_tree",
            "settings": ["show_disclosure_buttons"],
            "indent_offset": 12
        },
        // Sidebar tree Row padding space setting
        {
            "class": "sidebar_tree",
            "settings": {
                "show_disclosure_buttons": false,
                "row_padding_space": "normal"
            },
            "row_padding": [
                20, // if not disclosure, need more space because o Open files close/dirty buttons
                4, // 3 if font size sidebar_label is not 12
                8,
                4
            ]
        },
        {
            "class": "sidebar_tree",
            "settings": {
                "show_disclosure_buttons": true,
                "row_padding_space": "normal"
            },
            "row_padding": [
                8,
                4 // 3 if font size sidebar_label is not 12
            ]
        },
        {
            "class": "sidebar_tree",
            "settings": {
                "show_disclosure_buttons": false,
                "row_padding_space": "large"
            },
            "row_padding": [
                20,
                6,
                8,
                6
            ]
        },
        {
            "class": "sidebar_tree",
            "settings": {
                "show_disclosure_buttons": true,
                "row_padding_space": "large"
            },
            "row_padding": [
                8,
                6
            ]
        },
        /** Tree row */
        // Sidebar row
        {
            "class": "tree_row",
            "layer0.opacity": 0,
            "layer0.inner_margin": [
                8,
                4
            ]
        },
        // Tree row Mix and Digital
        {
            "class": "tree_row",
            "settings": {
                "light_theme_style": ["mix", "digital"]
            },
            "layer0.texture": "Theme - Treble/textures/sidebar/selected-row-round.png"
        },
        // Tree row Analog
        {
            "class": "tree_row",
            "settings": {
                "light_theme_style": "analog"
            },
            "layer0.texture": "Theme - Treble/textures/sidebar/selected-row-square.png"
        },
        // Tree row hover
        // Not seeing changes
        {
            "class": "tree_row",
            "attributes": [
                "selectable",
                "hover"
            ],
            "layer0.opacity": 1,
            "layer0.tint": "var(tree-row-hover)"
        },
        // Tree row selected
        {
            "class": "tree_row",
            "attributes": [
                "selected"
            ],
            "layer0.opacity": 1
            // "layer0.tint": "color(var(white1) a(0.1))"
        },
        // Tree row selected Accent color Blue
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "blue"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-blue)"
        },
        // Tree row selected Accent color Purple
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "purple"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-purple)"
        },
        // Tree row selected Accent color Pink
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "pink"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-pink)"
        },
        // Tree row selected Accent color Red
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "red"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-red)"
        },
        // Tree row selected Accent color Orange
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "orange"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-orange)"
        },
        // Tree row selected Accent color Yellow
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "yellow"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-yellow)"
        },
        // Tree row selected Accent color Green
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "green"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-green)"
        },
        // Tree row selected Accent color Grayscale
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "grayscale"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-white-selected-row)"
        },
        // Tree row selected Accent color Color-scheme
        {
            "class": "tree_row",
            "settings": {
                "accent_color": "color-scheme"
            },
            "attributes": [
                "selected"
            ],
            "layer0.tint": "var(accent-color-scheme)"
        },
        /** Sidebar heading */
        {
            "class": "sidebar_heading",
            "case": "title",
            "fg": "var(heading)",
            "font.size": "var(font-size-sidebar-heading)",
            "font.bold": true
        },
        // Sidebar heading Theme opacity setting
        {
            "class": "sidebar_heading",
            "settings": {
                "theme_opacity": "high"
            },
            "opacity": "var(heading-opacity)"
        },
        {
            "class": "sidebar_heading",
            "settings": {
                "theme_opacity": "medium"
            },
            "opacity": "var(heading-opacity-medium)"
        }, 
        {
            "class": "sidebar_heading",
            "settings": {
                "theme_opacity": "low"
            },
            "opacity": "var(heading-opacity-low)"
        },
        /** File system entry */
        {
            "class": "file_system_entry",
            "content_margin": [
                0,
                0,
                2,
                0
            ]
        },
        {
            "class": "file_system_entry",
            "settings": {
                "wide_scroll_bars": true
            },
            "content_margin": [
                0,
                0,
                9,
                0
            ]
        },
        {
            "class": "file_system_entry",
            "parents": [{"class": "scroll_area_control", "attributes": ["scrollable"]}],
            "settings": ["!overlay_scroll_bars"],
            "content_margin": 0
        },
        /** Sidebar label */
        {
            "class": "sidebar_label",
            // "font.size": 12,
            "font.size": "var(font-size-sidebar-label)",
            "fg": "var(label-normal)"
        },
        // Sidebar label Theme opacity setting
        {
            "class": "sidebar_label",
            "settings": {
                "theme_opacity": "high"
            },
            "opacity": "var(label-normal-opacity)"
        },
        {
            "class": "sidebar_label",
            "settings": {
                "theme_opacity": "medium"
            },
            "opacity": "var(label-normal-opacity-medium)"
        },
        {
            "class": "sidebar_label",
            "settings": {
                "theme_opacity": "low"
            },
            "opacity": "var(label-normal-opacity-low)"
        },
        // Sidebar label hover
        {
            "class": "sidebar_label",
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover"
                    ]
                }
            ],
            "fg": "var(label-hover)",
            "opacity": "var(label-hover-opacity)"
        },
        {
            "class": "sidebar_label", // works only if in user settings, bold_folder_labels is true
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "expandable"
                    ]
                }
            ],
            "settings": [
                "bold_folder_labels"
            ],
            "font.bold": true
        },
        // Sidebar label selected
        {
            "class": "sidebar_label",
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-dark)",
            "opacity": "var(label-selected-opacity)"
        },
        // Sidebar label selected Accent color Yellow
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "yellow"
            },
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-light)"
        },
        // Sidebar label selected Accent color Green
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "green"
            },
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-dark)"
        },
        // Sidebar label selected Accent color Grayscale
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "grayscale"
            },
            "parents": [
                // {
                //     "class": "window", 
                //     "attributes": [
                //         "file_light"
                //     ]
                // },
                {
                    "class": "tree_row",
                    "attributes": [
                        "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-light)"
        },
        // Sidebar label hover and selected
        {
            "class": "sidebar_label",
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover", "selected"
                    ]
                }
            ],
            "fg": "var(label-hover-selected-dark)",
            "opacity": "var(label-hover-selected-opacity)"
        },
        // Sidebar label hover and selected Accent color Yellow
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "yellow"
            },
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover", "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-light)"
        },
        // Sidebar label hover and selected Accent color Green
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "green"
            },
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover", "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-dark)"
        },
        // Sidebar label hover and selected Accent color Grayscale
        {
            "class": "sidebar_label",
            "settings": {
                "accent_color": "grayscale"
            },
            "parents": [
                // {
                //     "class": "window", 
                //     "attributes": [
                //         "file_light"
                //     ]
                // },
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover", "selected"
                    ]
                }
            ],
            "fg": "var(label-selected-light)"
        },
        // Sidebar label vcs status
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_ignored_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["ignored"]}],
            "color": "var(vcs-ignored)"
        },
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_untracked_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["untracked"]}],
            "color": "var(vcs-untracked)"
        },
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_tracked_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["modified"]}],
            "color": "var(vcs-modified)"
        },
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_tracked_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["added"]}],
            "color": "var(vcs-added)"
        },
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_tracked_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["staged"]}],
            "color": "var(vcs-staged)"
        },
        {
            "class": "sidebar_label",
            "settings": ["show_sidebar_untracked_label_colors"],
            "parents": [{"class": "file_system_entry", "attributes": ["unmerged"]}],
            "color": "var(vcs-unmerged)"
        },
        // Added - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_tracked_label_colors": true,
                "accent_color": ["blue", "purple", "pink", "red", "orange"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["added"]}
            ],
            "color": "var(vcs-added-lighter-tone)"
        },
        // Ignored - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_ignored_label_colors": true,
                "accent_color": ["blue", "purple", "pink", "red", "orange"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["ignored"]}
            ],
            "color": "var(vcs-ignored-lighter-tone)"
        },
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_ignored_label_colors": true,
                "accent_color": ["yellow", "green"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["ignored"]}
            ],
            "color": "var(vcs-ignored-darker-tone)"
        },
        // Modified - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_tracked_label_colors": true,
                "accent_color": ["yellow", "green"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["modified"]}
            ],
            "color": "var(vcs-modified-darker-tone)"
        },
        // Staged - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_tracked_label_colors": true,
                "accent_color": ["blue", "purple", "pink", "red", "orange"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["staged"]}
            ],
            "color": "var(vcs-staged-lighter-tone)"
        },
        // Unmerged - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_untracked_label_colors": true,
                "accent_color": ["pink", "red", "orange", "color-scheme"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["unmerged"]}
            ],
            "color": "var(vcs-unmerged-darker-tone)"
        },
        // Untracked - Sidebar label vcs status Accent color
        {
            "class": "sidebar_label",
            "settings": {
                "show_sidebar_untracked_label_colors": true,
                "accent_color": ["pink", "red", "orange", "yellow", "green"]
            },
            "parents": [
                {"class": "tree_row", "attributes": ["selected"]},
                {"class": "file_system_entry", "attributes": ["untracked"]}
            ],
            "color": "var(vcs-untracked-darker-tone)"
        },
        /** Close button */
        {
            "class": "close_button",
            "content_margin": 8,
            "layer0.opacity": 0, // showing only on hover
            "layer0.texture": "Theme - Treble/textures/sidebar/open-file-x.png",
            "layer0.inner_margin": 0
        },
        // Close button Tree row hover
        {
            "class": "close_button",
            "parents": [
                {
                    "class": "tree_row", 
                    "attributes": [
                        "hover"
                    ]
                }
            ],
            "layer0.opacity": "var(sidebar-icon-opacity-hover)",
            "layer0.tint": "var(sidebar-close-button-light)"
        },
        // Close button dirty
        {
            "class": "close_button",
            "attributes": [
                "dirty"
            ],
            "layer0.opacity": 0, // showing only on hover
            "layer0.texture": "Theme - Treble/textures/sidebar/open-file-dirty.png"
        },
        {
            "class": "close_button",
            "attributes": [
                "dirty"
            ],
            "parents": [
                {
                    "class": "tree_row",
                    "attributes": [
                        "hover"
                    ]
                }
            ],
            "layer0.tint": "var(sidebar-dirty-button)",
            "layer0.opacity": "var(sidebar-icon-opacity-hover)"
        },
        // Close button hover
        {
            "class": "close_button",
            "attributes": [
                "hover"
            ],
            "layer0.tint": "var(sidebar-close-button-light)",
            "layer0.opacity": "var(sidebar-icon-opacity-hover)",
            "layer0.texture": "Theme - Treble/textures/sidebar/open-file-x.png"
        },
        // Close button Tree row hover and selected
        {
            "class": "close_button",
            "parents": [
                {
                    "class": "tree_row", 
                    "attributes": [
                        "hover",
                        "selected"
                    ]
                }
            ],
            "layer0.tint": "var(sidebar-close-button-dark)",
            "layer0.opacity": "var(sidebar-icon-opacity-hover-selected)"
        },
        // Close button Tree row hover and selected Accent color Yellow
        {
            "class": "close_button",
            "settings": {
                "accent_color": "yellow"
            },
            "parents": [
                {
                    "class": "tree_row", 
                    "attributes": [
                        "hover",
                        "selected"
                    ]
                }
            ],
            "layer0.tint": "var(sidebar-close-button-light)"
        },
        // Close button Tree row hover and selected Accent color Green
        {
            "class": "close_button",
            "settings": {
                "accent_color": "green"
            },
            "parents": [
                {
                    "class": "tree_row", 
                    "attributes": [
                        "hover",
                        "selected"
                    ]
                }
            ],
            "layer0.tint": "var(sidebar-close-button-dark)"
        },
        // Close button Tree row hover and selected Accent color Grayscale
        {
            "class": "close_button",
            "settings": {
                "accent_color": "grayscale"
            },
            "parents": [
                // {
                //     "class": "window",
                //     "attributes": [
                //         "file_light"
                //     ]
                // },
                {
                    "class": "tree_row", 
                    "attributes": [
                        "hover",
                        "selected"
                    ]
                }
            ],
            "layer0.tint": "var(sidebar-close-button-light)"
        },
        /** Disclosure button control */
        // icon sidebar left of icon folder
        // hidden disclosure button, code from D'Aneo Theme https://github.com/SublimeText/Theme-DAneo/
        {
            "class": "disclosure_button_control",
            "content_margin": 0,
            "layer0.tint": null,
            "layer0.opacity": 0,
            "layer0.texture": ""
        },
        {
            "class": "disclosure_button_control",
            "settings": ["show_disclosure_buttons"],
            "content_margin": [7, 8],
            "layer0.tint": "var(sidebar-disclosure-button)",
            "layer0.texture": "Theme - Treble/textures/sidebar/disclosure-closed.png",
            "layer0.inner_margin": 0
        },
        // Disclosure button control Theme opacity setting
        {
            "class": "disclosure_button_control",
            "settings": {
                "show_disclosure_buttons": true,
                "theme_opacity": "high"
            },
            "layer0.opacity": "var(sidebar-icon-opacity-normal)"
        },
        {
            "class": "disclosure_button_control",
            "settings": {
                "show_disclosure_buttons": true,
                "theme_opacity": "medium"
            },
            "layer0.opacity": "var(sidebar-icon-opacity-medium)"
        },
        {
            "class": "disclosure_button_control",
            "settings": {
                "show_disclosure_buttons": true,
                "theme_opacity": "low"
            },
            "layer0.opacity": "var(sidebar-icon-opacity-low)"
        },
        // Disclosure button control hover selected
        {
            "class": "disclosure_button_control",
            "settings": ["show_disclosure_buttons"],
            "parents": [{"class": "tree_row", "attributes": ["hover"]}],
            "layer0.opacity": "var(sidebar-icon-opacity-hover)"
        },
        {
            "class": "disclosure_button_control",
            "settings": ["show_disclosure_buttons"],
            "parents": [{"class": "tree_row", "attributes": ["selected"]}],
            "layer0.opacity": "var(sidebar-icon-opacity-selected)"
        },
        {
            "class": "disclosure_button_control",
            "settings": ["show_disclosure_buttons"],
            "parents": [{"class": "tree_row", "attributes": ["selected", "hover"]}],
            "layer0.opacity": "var(sidebar-icon-opacity-hover-selected)"
        },
        /** Disclosure button control open */
        {
            "class": "disclosure_button_control",
            "settings": ["show_disclosure_buttons"],
            "attributes": ["expanded"],
            "layer0.texture": "Theme - Treble/textures/sidebar/disclosure-opened.png"
        },
        // Disclosure button control Vcs status ignored
        // not working
        // {
        //     "class": "disclosure_button_control",
        //     "settings": ["show_disclosure_buttons"],
        //     "parents": [
        //         {"class": "file_system_entry", "attributes": ["ignored"]}
        //     ],
        //     "layer0.opacity": 0.2,
        // },
        /** Icon folder */
        // hidden sidebar folder icons, code from D'Aneo Theme https://github.com/SublimeText/Theme-DAneo/
        {
            "class": "icon_folder",
            "content_margin": 0,
            "layer0.opacity": 0,
            "layer0.texture": ""
        },
        // Icon folder Mix and Digital - Solid setting
        {
            "class": "icon_folder",
            "settings": {
                "light_theme_style": ["mix", "digital"],
                "show_sidebar_icons": true,
                "folder_icon_style": "solid"
            },
            "layer0.texture": "Theme - Treble/textures/sidebar/folder-closed-solid.png"
        },
        // Icon folder Mix and Digital - Outline setting
        {
            "class": "icon_folder",
